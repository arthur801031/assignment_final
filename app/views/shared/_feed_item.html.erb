<li id="<%= feed_item.id %>">
 <span></span>
  <span class="content">Title: <%= feed_item.title %></span>
  <span class="content">Description: <%= feed_item.description %></span>
   <span class="content">Due Date: <%= feed_item.duedate %></span>
   <span>
	<% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" }, 
                                     title: feed_item.title%>
  <% end %>
	</span>
</li>